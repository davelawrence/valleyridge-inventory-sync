    {
      "Id": "ProcessInventoryTriggerXLS",
      "LambdaFunctionArn": "arn:aws:lambda:us-east-1:413362489612:function:valleyridge-process-inventory",
      "Events": ["s3:ObjectCreated:*"],
      "Filter": {
        "Key": {
          "FilterRules": [
            {
              "Name": "prefix",
              "Value": "incoming/"
            },
            {
              "Name": "suffix",
              "Value": ".xls"
            }
          ]
        }
      }
    },
    {
      "Id": "ProcessInventoryTriggerXLSX",
      "LambdaFunctionArn": "arn:aws:lambda:us-east-1:413362489612:function:valleyridge-process-inventory",
      "Events": ["s3:ObjectCreated:*"],
